{
  "name": "react-text-selection-popover",
  "version": "1.2.4",
  "description": "A popover component positioned according to the current selection in a contenteditable element",
  "author": "juliankrispel",
  "license": "MIT",
  "repository": "juliankrispel/react-text-selection-popover",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "flow": "flow",
    "lint": "eslint src",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "rollup -c",
    "precommit": "lint-staged && yarn run lint && yarn run flow && yarn run test",
    "prettier": "prettier src/**/*.js example/src/**/*.js --write",
    "start": "rollup -c -w",
    "prepare": "yarn run build",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "invariant": "2.2.4",
    "lodash.debounce": "4.0.8",
    "react-event-listener": "0.6.6",
    "react-measure": "2.1.2",
    "react-window-dimensions": "1.0.2"
  },
  "peerDependencies": {
    "react": "^16.3.0",
    "react-dom": "16.3.0"
  },
  "devDependencies": {
    "prop-types": "^15.5.4",
    "babel-core": "^6.26.0",
    "rollup-plugin-copy": "0.2.3",
    "babel-eslint": "^8.2.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.1.4",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "flow-bin": "^0.76.0",
    "gh-pages": "^1.1.0",
    "globby": "^8.0.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "^1.1.1",
    "rollup": "^0.54.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-plugin-peer-deps-external": "^2.0.0",
    "rollup-plugin-postcss": "^1.1.0",
    "rollup-plugin-url": "^1.3.0"
  },
  "files": [
    "dist",
    "lib"
  ]
}
